    "a = 100\n",
    "prime = []\n",
    "\n",
    "for i in range(2, a + 1):\n",
    "    for j in range(2, int(i ** 0.5) + 1):\n",
    "         if i%j == 0:\n",
    "            break\n",
    "    else:\n",
    "        prime.append(i)\n",
    "\n",
    "print(prime)"
