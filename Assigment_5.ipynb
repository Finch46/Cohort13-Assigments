   ],
   "source": [
    "a = int(input(\"Enter number :\"))\n",
    "\n",
    "counter = 0\n",
    "\n",
    "for i in range(1, a + 1) :\n",
    "    if a % i == 0 :\n",
    "        counter += 1\n",
    "        \n",
    "if (a == 0) or (a == 1) or (counter >= 3) :\n",
    "    print(a, \" is not prime number.\")\n",
    "    \n",
    "else:\n",
    "    print(a, \" is prime number.\")"
   ]
